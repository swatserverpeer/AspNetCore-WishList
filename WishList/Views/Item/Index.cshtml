@using WishList.Models
@model List<Item>


    <h1>Wishlist</h1>
    <a asp-action="create">Add item</a>
    <ul>
        @foreach (var item in Model.Item)
       {
           <li>
                @item.Description;
                <a asp-action="delete" asp-route-id="@item.id">delete</a>
           </li>
       }

    </ul>